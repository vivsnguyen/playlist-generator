<!-- for add to playlist to have songs show on screen
<div #song-display></div>


 <script>
function showSongsToChoose(evt) {
    evt.preventDefault();

    let url = "/add-to-playlist";
    let formData = {
        "playlist_title" : $("#playlist-field").val(),
        "artist_name": $("#artist-field").val()
    };

    $.post(url, formData, (res) => {
        console.log(res)
      $('#song-display').html(res));
});
}

$("#playlist-form").on('submit', showSongsToChoose);
</script> -->
